<% @messages.each do |message| %>
  <div class="row">
    <div class="col s12 m4 l8">
      <strong><%= message.user.fullname %></strong>
      <br>
      <div class="chip">
        <%= message.content %>
        <span class="pull-right"> -<%= message.message_time %></span>
      </div>
    </div>
<% end %>
